{
    "manifest_version": 3,
    "name": "Odoo github extension",
    "version": "1.0",
    "description": "Add few buttons to github page for odoo",
    "icons": {
      "16": "icon.png",
      "48": "icon.png",
      "128": "icon.png"
    },
    "content_scripts": [
      {
        "matches": ["https://github.com/odoo/*"],
        "js": ["script.js"],
        "run_at": "document_idle"
      }
    ],
    "permissions": ["scripting", "tabs"]
  }
  